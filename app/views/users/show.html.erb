<h1>My Page</h1>
<% if current_user %>
 Logged in as <%= current_user.username %>
<% end %>
<ul>

<h2>Username:<%= @user.username%></h2>

<li>
<h2>My Cookbook</h2>
<% @user.cook_books.each do |cook_book|%></p>
    <p><%= cook_book.name %>
<%end%>
</li>

<li>
<h2>All Recipes Submitted by Me</h2>
<% @user.recipes.each do |recipe|%></p>
    <p><%= recipe.name %>
<%end%>
</li> 
 
</ul>

<%= button_to "Log out", session_path("current"),  method:"delete", class: "btn btn-success" %>